	"private": true,
	"scripts": {
		"dev": "next dev --turbopack -p 3001",
		"debugdev": "bun run postinstall; NODE_OPTIONS='--inspect' next dev --turbopack -p 3001",
		"build": "bun run postinstall; next build",
		"start": "next start -p 3000",
		"bio_lint": "npx biome check --fix ./src",
		"pretty": "npx biome format --write ./src",
		"postinstall": "bun run sql:generate",
		"dbpush": "bun run sql:dbpush",
		"sql:generate": "npx prisma generate",
		"sql:dbpush": "npx prisma db push --schema prisma/schema.prisma",
		"depcheck": "npx depcheck .",
		"precommit": "lint-staged",
		"commit": "clear && npx git-cz",
		"lint": "next lint; eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\""
