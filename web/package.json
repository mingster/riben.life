  "scripts": {
    "dev": "next dev -p 3000",
    "debugdev": "yarn postinstall; NODE_OPTIONS='--inspect' next dev -p 3000",
    "build": "yarn postinstall; next build",
    "start": "next start",
    "lint": "next lint; eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "pretty": "prettier --write \"./**/*.{ts,tsx,js,jsx,json}\" --config ./.prettierrc",
    "biome": "npx @biomejs/biome format --write ./src",
    "biolint": "npx @biomejs/biome check --write ./src",
    "postinstall": "yarn sql:generate; yarn mongo:generate",
    "dbpush": "yarn sql:dbpush; yarn mongo:dbpush",
    "sql:generate": "npx prisma generate",
    "sql:dbpush": "npx prisma db push --schema prisma/schema.prisma",
    "mongo:generate": "prisma generate --schema prisma-mongo/schema.prisma",
    "mongo:dbpush": "npx prisma db push --schema prisma-mongo/schema.prisma"
  },
