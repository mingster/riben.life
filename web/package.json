  "name": "riben.life",
  "version": "0.1.16",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev --turbo -p 3001",
    "dev:node": "next dev --turbo -p 3001",
    "debugdev": "bun run postinstall; NODE_OPTIONS='--inspect' bun --bun next dev --turbo -p 3001",
    "dev:webpack": "bun --bun next dev -p 3001",
    "build:perf": "node ../bin/build-performance.js",
    "build:analyze": "ANALYZE=true bun run build",
    "build:fast": "bun run postinstall:parallel && next build",
    "build:optimized": "node scripts/build-optimize.js optimized",
    "build:parallel": "node scripts/build-optimize.js parallel",
    "build:production": "node scripts/build-optimize.js production",
    "build:fast-optimized": "node scripts/build-optimize.js fast",
    "build": "bun run postinstall && bun run next build",
    "start": "bun --bun next start -p 3001",
    "install:platform": "bun run ./bin/install.ts",
    "bio_lint": "npx biome check --fix ./src",
    "format": "npx biome format --write ./src",
    "postinstall": "bun run sql:generate",
    "dbpush": "bun run sql:dbpush",
    "sql:generate": "npx prisma generate",
    "sql:dbpush": "npx prisma db push --schema prisma/schema.prisma",
    "depcheck": "npx depcheck .",
    "precommit": "lint-staged",
    "commit": "clear && npx git-cz",
    "lint": "next lint; eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "cleanup:console": "bun run scripts/cleanup-console-logs.ts",
    "optimize:cache": "node scripts/cache-optimize.js"
  },
