  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3000",
    "debugdev": "bun run postinstall; NODE_OPTIONS='--inspect' next dev --turbopack -p 3000",
    "build": "bun run postinstall; next build",
    "start": "next start -p 3000",
    "lint": "next lint; eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "biolint": "npx @biomejs/biome check --fix ./src/**/*.{ts,tsx,js,jsx,json}",
    "pretty_old": "prettier --write \"./**/*.{ts,tsx,js,jsx,json}\" --config ./.prettierrc",
    "pretty": "npx @biomejs/biome format --write ./src",
    "postinstall": "bun run sql:generate; bun run mongo:generate",
    "dbpush": "bun run sql:dbpush; bun run mongo:dbpush",
    "sql:generate": "npx prisma generate",
    "sql:dbpush": "npx prisma db push --schema prisma/schema.prisma",
    "mongo:generate": "npx prisma generate --schema prisma-mongo/schema.prisma",
    "mongo:dbpush": "npx prisma db push --schema prisma-mongo/schema.prisma",
    "depcheck": "npx depcheck ."
  },
