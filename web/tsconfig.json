{
  "compilerOptions": {
    "target": "ES2022",
    "module": "es2020",
    //"module": "esnext",
    "lib": [
      "dom",
      "ES2022",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/(store)/[storeId]/[tableId]/page.tsx-old",
    "src/components/store-require-prepaid.-prompt.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}